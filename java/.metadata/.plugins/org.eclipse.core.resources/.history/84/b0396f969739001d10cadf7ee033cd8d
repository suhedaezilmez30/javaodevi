
public class Main {

	public static void main(String[] args) {
		//220-284
		int number=220;
		int total=0;
		
		for(int i=1; i<220; i++) {
			if(number%i==0) {
				total=total+i;
			}
			
		}
		if(total==number) {
			System.out.println("arkadaş sayı");
		}
		else {
			System.out.println("arkadaş sayı değildir");
		}
	}

}
