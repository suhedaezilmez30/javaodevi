package enBuyukSayiBulma;

public class Main {

	public static void main(String[] args) {
		int sayi1 = 10 ;
		int sayi2= 12;
		int sayi3 = 5 ;
		int enBuyuk=sayi1;
		if( enBuyuk<sayi2);
		{
			enBuyuk = sayi2;
			
		}
		if(sayi1<sayi3)
		{
			enBuyuk = sayi3;
					
		}
		System.out.println(enBuyuk);

	}

}
